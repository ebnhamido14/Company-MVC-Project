@model UserViewModel
@{
    ViewData["Title"] = "Delete";
}
<h1>Are you Sure You Want To Delete Employee @Model.FName @Model.LName</h1>
<form asp-action="ConfirmDelete">
    <partial name="PartialViews/DetailsDeletePartialView" />
    <input type="Submit" value="Delete" asp-route-id="@Model.Id" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-light">Cancel</a>
</form>
